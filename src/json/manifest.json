{
    "manifest_version": 2,
    "name":             "ProxyParser",
    "description":      "Makes json string in one click from tables of public proxy lists",
    "version":          "1.0",
    "browser_action":   {
        "default_icon":  "icon.png",
        "default_popup": "popup.html"
    },
    "permissions":      [
        "activeTab",
        "storage"
    ]
}